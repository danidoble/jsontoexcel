{
    "name": "danidoble/jsontoexcel",
    "version": "v1.0.0",
    "description": "make an excel from json string",
    "type": "library",
    "require": {
        "php": "^8.2",
        "phpoffice/phpspreadsheet": "^2.0",
        "danidoble/phpobject": "^v1.0"
    },
    "require-dev": {
        "laravel/pint": "^v1.15",
        "pestphp/pest": "^2.34",
        "pestphp/pest-plugin-drift": "^2.6"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Danidoble\\Jsontoexcel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "danidoble",
            "email": "ddanidoble@gmail.com"
        }
    ],
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
