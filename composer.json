{
    "name": "danidoble/jsontoexcel",
    "version": "v0.0.1",
    "description": "make an excel from json string",
    "type": "library",
    "require": {
        "php": "^8.1",
        "phpoffice/phpspreadsheet": "^1.29",
        "danidoble/phpobject": "^v0.1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.2.2",
        "friendsofphp/php-cs-fixer": "^v3.17"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Danidoble\\Jsontoexcel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "danidoble",
            "email": "ddanidoble@gmail.com"
        }
    ],
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
